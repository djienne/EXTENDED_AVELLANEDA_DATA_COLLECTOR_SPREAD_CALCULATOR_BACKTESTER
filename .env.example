# Extended DEX Configuration
# Copy this file to .env and fill in your credentials
# NEVER commit .env to version control!

# API key from app.extended.exchange
# Use either API_KEY or EXTENDED_API_KEY (both are supported)
API_KEY=your_api_key_here

# Starknet keys (L2 keys)
STARK_PUBLIC=0x0...
STARK_PRIVATE=0x0...

# Your L2 vault number / collateral position ID
VAULT_NUMBER=123456

# Environment (mainnet or testnet)
EXTENDED_ENV=mainnet
